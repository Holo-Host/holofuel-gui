  "scripts": {
    "start-agent1": "PORT=8800 REACT_APP_NODE_PORT=3000 REACT_APP_DNA_INSTANCE=test-instance react-scripts-ts start",
    "start-agent2": "PORT=9300 REACT_APP_NODE_PORT=3100 REACT_APP_DNA_INSTANCE=test-instance react-scripts-ts start",
    "start-web": "cross-env NODE_ENV=development webpack-dev-server --config configs/webpack.config.base.js",
    "build": " REACT_APP_NODE_PORT=3000 REACT_APP_DNA_INSTANCE=holofuel-instance react-scripts-ts build && node copy-to-ui.js",
    "build-holo": "REACT_APP_NODE_PORT=48080 REACT_APP_DNA_INSTANCE=holofuel-holo REACT_APP_KV_STORE_HAPPHASH=QmXh734btDo1C6pChxkQU1LDk6XTxtrn5YmrFvq2hGiwtF react-scripts-ts build && node copy-to-ui.js",
    "build-dll": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.renderer.dev.dll.babel.js --colors",
    "build-prod": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.main.prod.babel.js --colors",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "coverage": "react-scripts-ts test --env=jsdom --coverage --collectCoverageFrom=!src/**/*.d.ts --collectCoverageFrom=!src/**/*.stories.tsx --collectCoverageFrom=!src/**/data/*.ts --collectCoverageFrom=src/**/*.ts --collectCoverageFrom=src/**/*.tsx",
    "release:files": "node copy-release.js",
    "tslint": "tslint --project ./tsconfig.json",
    "tslint:fix": "tslint --project ./tsconfig.json --fix",
    "watch": "tsc -p tsconfig.test.json -w "
